{
  "name": "aifree-shared-types",
  "version": "1.0.0",
  "description": "A repository of shared TypeScript types and interfaces for use in various projects.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./src/*.js": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "typescript",
    "shared",
    "types",
    "interfaces"
  ],
  "author": "Abdallah jguirim",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.0.0"
  }
}